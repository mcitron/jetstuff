void generate_matching_efficiencies() {

  //TFile * file = new TFile("histograms-ttbar_13TeV_PU40_25ns_test_skimmed_v2_5GeVseed_test.root","READ");
  //TFile * file = new TFile("histograms-neutrinogun_13TeV_PU40_25ns_1-10_skimmed_v2_allevents_v1.root","READ");

  //TFile * file = new TFile("../histograms-ttbar_13TeV_noPU_v2_allevents.root","READ");
  TFile * file = new TFile("../histograms-ttbar_13TeV_PU40_50ns_skimmed_v3_allevents.root","READ");

  TH1D * hist1 = (TH1D*)((TH1D*)file->Get("demo/ak4tt_ak4genjetsp/col2_alljet_pt")->Clone());
  TH1D * hist2 = (TH1D*)((TH1D*)file->Get("demo/ak4tt_ak4genjetsp/col2_matched_alljet_pt")->Clone());

  TH1D * hist3 = (TH1D*)((TH1D*)file->Get("demo/L14400_ak4tt/col2_alljet_pt")->Clone());
  TH1D * hist4 = (TH1D*)((TH1D*)file->Get("demo/L14400_ak4tt/col2_matched_alljet_pt")->Clone());

  TH1D * hist5 = (TH1D*)((TH1D*)file->Get("demo/L14300_ak4tt/col2_alljet_pt")->Clone());
  TH1D * hist6 = (TH1D*)((TH1D*)file->Get("demo/L14300_ak4tt/col2_matched_alljet_pt")->Clone());

  hist1->Sumw2();
  hist2->Sumw2();
  hist3->Sumw2();
  hist4->Sumw2();
  hist5->Sumw2();
  hist6->Sumw2();
  ////hist1->Rebin(10);
  ////hist2->Rebin(10);
  //hist3->Rebin(10);
  hist2->Divide(hist1);
  hist4->Divide(hist3);
  hist6->Divide(hist5);
  //hist3->Divide(hist1);
  /*
  TH1D * hist4 = (TH1D*)((TH1D*)file->Get("demo/global_histograms/genjet_eta_nomunu")->Clone());
  TH1D * hist5 = (TH1D*)((TH1D*)file->Get("demo/global_histograms/genjet_eta_nomunu_match_L1")->Clone());
  TH1D * hist6 = (TH1D*)((TH1D*)file->Get("demo/global_histograms/genjet_eta_nomunu_match_ak4tt")->Clone());

  hist4->Sumw2();
  hist5->Sumw2();
  hist6->Sumw2();

  hist5->Divide(hist4);
  hist6->Divide(hist4);
  */
  TFile * ofile = new TFile("output-file-matching-ttbar13pu40_50ns.root","RECREATE");
  //hist1->Draw();
  ofile->cd();
  hist2->SetName("one");
  hist2->Write();
  hist4->SetName("two");
  hist4->Write();
  hist6->SetName("three");
  hist6->Write();
  //hist3->Write();
  //hist5->Write();
  //hist6->Write();
  ofile->Close();

  return;

}
